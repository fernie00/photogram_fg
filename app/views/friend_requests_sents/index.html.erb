<div class="page-header">
  <h1>
    Friend requests sents
    <a href="/friend_requests_sents/new" class="btn btn-lg btn-success">New Friend requests sent</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Sender</th>
        <th>Recipient</th>
        <th>Actions</th>
      </tr>

      <% @friend_requests_sents.each do |friend_requests_sent| %>
      <tr>
        <td><%= friend_requests_sent.sender_id %></td>
        <td><%= friend_requests_sent.recipient_id %></td>
        <td>
          <a href="/friend_requests_sents/<%= friend_requests_sent.id %>" class="btn btn-primary">Show</a>
          <a href="/friend_requests_sents/<%= friend_requests_sent.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_friend_requests_sent/<%= friend_requests_sent.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
